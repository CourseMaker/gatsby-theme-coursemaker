{
    "extends": ["wesbos"],
    "rules": {
        "no-console": 2,
        "import/no-unresolved": 0,
        "react/display-name": 0,
        "react/prop-types": 0,
        "camelcase": "off",
        "prettier/prettier": [
            "warn",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 4
            }
        ]
    }
}