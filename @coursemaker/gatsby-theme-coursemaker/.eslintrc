{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "strict": 0,
        "no-console": "warn",
        "quotes": [
            "warn",
            "single"
        ],
        "prettier/prettier": "warn",
        "react/prop-types": "warn",
        "no-unused-vars": "warn"
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}